	Doi = {DOI: 10.1016/j.tins.2006.06.003},
	Issn = {0166-2236},
	Journal = {Trends in Neurosciences},
	Note = {Nature and nurture in brain development and neurological disorders},
	Number = {7},
	Pages = {382 -- 390},
	Title = {Dynamic Auditory Processing, Musical Experience and Language Development},
	Url = {http://www.sciencedirect.com/science/article/B6T0V-4K8S5GB-1/2/b6c286abf89ab01869b4c2d3ba2ef9ef},
